    "quagga": "^0.12.1",
    "sass": "^1.27.0",
    "v8-compile-cache": "^2.1.0",
    "vue": "^2.6.11",
    "vue-3d-model": "^1.3.1",
    "vue-quaggajs": "^1.0.12",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.4.0",
    "@vue/cli-plugin-eslint": "^4.4.0",
    "@vue/cli-plugin-router": "^4.4.0",
    "@vue/cli-plugin-unit-jest": "^3.12.1",
    "@vue/cli-plugin-vuex": "^4.4.0",
